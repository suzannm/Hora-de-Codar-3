programa {
  funcao inicio() {
    real i, valor, numerosMeio=0, numerosFora=0

    para (i= 1; i<=10; i++){
      escreva("Digite o ", i,"* valor: ")
      leia(valor)
      se(valor>=24 e valor<=72){
        numerosMeio = 1+numerosMeio
      } senao {
        numerosFora = 1+numerosFora
      }
    }
    escreva("Valores entre o intervalo 24 a 72 são: ", numerosMeio,". Valores fora do intervalo são: ",numerosFora,".")

  }
}
