programa {
  funcao inicio() {
    real media, soma=0, numero, notas=0

    para (inteiro i=1; i<=6; i++){
      escreva("Digite a ", i, "* nota: ")
      leia(numero)
      soma = numero + soma
      notas += 1
    }

    media = soma / notas

    escreva("A nota média do aluno é: ", media, ".")

  }
}
